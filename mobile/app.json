{
  "expo": {
    "name": "FastFeet",
    "slug": "fast-feet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7D40E7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ronnyacacio.fastfeet",
      "adaptiveIcon": {
        "backgroundColor": "#7D40E7",
        "foregroundImage": "./assets/icon.png"
      }
    },
    "githubUrl": "https://github.com/ronnyacacio/fast-feet/mobile"
  }
}
